	/** Returns a fast iterator (i.e., one that might reuse an entry object) when supported by the given {@code map}.
	*/
	SUPPRESS_WARNINGS_KEY_VALUE_UNCHECKED
	public static KEY_VALUE_GENERIC ObjectIterator<MAP.Entry KEY_VALUE_GENERIC> fastIterator(MAP KEY_VALUE_GENERIC map) {
		ObjectSet<MAP.Entry KEY_VALUE_GENERIC> entries = map.ENTRYSET();
		if (entries instanceof MAP.FastEntrySet) {
			return ((MAP.FastEntrySet KEY_VALUE_GENERIC) entries).fastIterator();
		}
		return entries.iterator();
	}

	/** Returns an iterable yielding a fast iterator (i.e., one that might reuse an entry object) when
	* supported by the given {@code map}. Useful in conjunction with, e.g., foreach loops.
	*/
	SUPPRESS_WARNINGS_KEY_VALUE_UNCHECKED
	public static KEY_VALUE_GENERIC ObjectIterable<MAP.Entry KEY_VALUE_GENERIC> fastIterable(MAP KEY_VALUE_GENERIC map) {
		ObjectSet<MAP.Entry KEY_VALUE_GENERIC> entries = map.ENTRYSET();
		if (entries instanceof MAP.FastEntrySet) {
			final MAP.FastEntrySet KEY_VALUE_GENERIC fastEntries = (MAP.FastEntrySet KEY_VALUE_GENERIC) entries;
			return new ObjectIterable<MAP.Entry KEY_VALUE_GENERIC>() {
				@Override
				public ObjectIterator<MAP.Entry KEY_VALUE_GENERIC> iterator() {
					return fastEntries.fastIterator();
				}
			};
		}
		return entries;
	}

