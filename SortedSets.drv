#if ! #keyclass(Reference)

	/** A class representing a sorted singleton. */

	protected static class SINGLETON extends Sets.SINGLETON implements SORTEDSET, java.io.Serializable, Cloneable {
	
		public static final long serialVersionUID = -7046029254386353129L;

		KEY_COMPARATOR comparator;

		private SINGLETON( final KEY_TYPE element, final KEY_COMPARATOR comparator ) {
			super( element );
			this.comparator = comparator;
		}

		private SINGLETON( final KEY_TYPE element ) {
			this( element, null );
		}

		final int compare( final KEY_TYPE k1, final KEY_TYPE k2 ) {
			return comparator == null ? KEY_CMP( k1, k2 ) : comparator.compare( k1, k2 );
		}

		public KEY_BIDI_ITERATOR iterator( KEY_TYPE from ) { 
			KEY_BIDI_ITERATOR i = (KEY_BIDI_ITERATOR)KEY_ITERATOR_METHOD(); 
			if ( compare( element, from ) <= 0 ) i.next();
			return i;
		}

		public Comparator comparator() { return comparator; }

		public STDSORTEDSET subSet( KEY_TYPE from, KEY_TYPE to ) { if ( compare( from, element ) <= 0 && compare( element, to ) <= 0 ) return this; else return Sets.EMPTY_SET; }
		public STDSORTEDSET headSet( KEY_TYPE to ) { if ( compare( element, to ) <= 0 ) return this; else return Sets.EMPTY_SET; }
		public STDSORTEDSET tailSet( KEY_TYPE from ) { if ( compare( from, element ) <= 0 ) return this; else return Sets.EMPTY_SET; }

		public KEY_TYPE FIRST() { return element; }
		public KEY_TYPE LAST() { return element; }

#if ! #keyclass(Object) && ! #keyclass(Reference)
		public Object first() { return KEY2OBJ( element ); }
		public Object last() { return KEY2OBJ( element ); }

		public Iterator iterator( Object from ) { return iterator( KEY2TYPE( from ) ); }

		public SortedSet subSet( Object from, Object to ) { return subSet( KEY2TYPE( from ), KEY2TYPE( to ) ); }
		public SortedSet headSet( Object to ) { return headSet( KEY2TYPE( to ) ); }
		public SortedSet tailSet( Object from ) { return tailSet( KEY2TYPE( from ) ); }
#endif
	}


