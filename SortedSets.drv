	protected static class SynchronizedSortedSet extends Sets.SYNCHRONIZED_SET implements SORTEDSET, java.io.Serializable {

		public static final long serialVersionUID = -7046029254386353129L;

		SORTEDSET sortedset;

		SynchronizedSortedSet( SORTEDSET s, Object sync ) {
			super( s, sync );
			sortedset = s;
		}

		SynchronizedSortedSet( SORTEDSET s ) {
			this( s, null );
		}

		public Comparator comparator() { synchronized( sync ) { return sortedset.comparator(); } }

		public STDSORTEDSET subSet( KEY_TYPE from, KEY_TYPE to ) { return new SynchronizedSortedSet( (SORTEDSET)sortedset.subSet( from, to ), sync ); }
		public STDSORTEDSET headSet( KEY_TYPE to ) { return new SynchronizedSortedSet( (SORTEDSET)sortedset.headSet( to ), sync ); }
		public STDSORTEDSET tailSet( KEY_TYPE from ) { return new SynchronizedSortedSet( (SORTEDSET)sortedset.tailSet( from ), sync ); }

		public KEY_BIDI_ITERATOR iterator( KEY_TYPE from ) { return sortedset.iterator( from ); }

		public KEY_TYPE FIRST() {  synchronized( sync ) { return sortedset.FIRST(); } }
		public KEY_TYPE LAST() {  synchronized( sync ) { return sortedset.LAST(); } }

#if ! #keyclass(Object) && ! #keyclass(Reference)
		public Object first() {  synchronized( sync ) { return sortedset.first(); } }
		public Object last() {  synchronized( sync ) { return sortedset.last(); } }

		public Iterator iterator( Object from ) { return sortedset.iterator( KEY2TYPE( from ) ); }

		public SortedSet subSet( Object from, Object to ) { return new SynchronizedSortedSet( (SORTEDSET)sortedset.subSet( from, to ), sync ); }
		public SortedSet headSet( Object to ) { return new SynchronizedSortedSet( (SORTEDSET)sortedset.headSet( to ), sync ); }
		public SortedSet tailSet( Object from ) { return new SynchronizedSortedSet( (SORTEDSET)sortedset.tailSet( from ), sync ); }
#endif
	}


	/** Returns a synchronized sorted set backed by the specified sorted set.
	 *
	 * @param s the sorted set to be "wrapped" in a synchronized sorted set.
	 * @return a synchronized view of the specified sorted set.
	 * @see java.util.Collections#synchronizedSortedSet(SortedSet)
	 */
	public static SORTEDSET synchronize( SORTEDSET s ) {	return new SynchronizedSortedSet( s ); }

	/** Returns a synchronized sorted set backed by the specified sorted set, using an assigned object to synchronize.
	 *
	 * @param s the sorted set to be "wrapped" in a synchronized sorted set.
	 * @param sync an object that will be used to synchronize the list access.
	 * @return a synchronized view of the specified sorted set.
	 * @see java.util.Collections#synchronizedSortedSet(SortedSet)
	 */

	public static SORTEDSET synchronize( SORTEDSET s, Object sync ) { return new SynchronizedSortedSet( s, sync ); }






